generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  id String @id @default(uuid())

  field_id String
  fields   Field[]
}

model Field {
  id String @id @default(uuid())

  game_id String
  game    Game   @relation(fields: [game_id], references: [id])
}
